{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "store": {
      "projectType": "library",
      "root": "projects/store",
      "sourceRoot": "projects/store/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/store/tsconfig.lib.json",
            "project": "projects/store/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/store/src/test.ts",
            "tsConfig": "projects/store/tsconfig.spec.json",
            "karmaConfig": "projects/store/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/store/tsconfig.lib.json",
              "projects/store/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "utils": {
      "projectType": "library",
      "root": "projects/utils",
      "sourceRoot": "projects/utils/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/utils/tsconfig.lib.json",
            "project": "projects/utils/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/utils/src/test.ts",
            "tsConfig": "projects/utils/tsconfig.spec.json",
            "karmaConfig": "projects/utils/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/utils/tsconfig.lib.json",
              "projects/utils/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "app-ochibooh": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/app-ochibooh",
      "sourceRoot": "projects/app-ochibooh/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/app-ochibooh",
            "index": "projects/app-ochibooh/src/index.html",
            "main": "projects/app-ochibooh/src/main.ts",
            "polyfills": "projects/app-ochibooh/src/polyfills.ts",
            "tsConfig": "projects/app-ochibooh/tsconfig.app.json",
            "aot": false,
            "assets": [
              "projects/app-ochibooh/src/favicon.ico",
              "projects/app-ochibooh/src/assets"
            ],
            "styles": [
              "projects/app-ochibooh/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/app-ochibooh/src/environments/environment.ts",
                  "with": "projects/app-ochibooh/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "app-ochibooh:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "app-ochibooh:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app-ochibooh:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/app-ochibooh/src/test.ts",
            "polyfills": "projects/app-ochibooh/src/polyfills.ts",
            "tsConfig": "projects/app-ochibooh/tsconfig.spec.json",
            "karmaConfig": "projects/app-ochibooh/karma.conf.js",
            "assets": [
              "projects/app-ochibooh/src/favicon.ico",
              "projects/app-ochibooh/src/assets"
            ],
            "styles": [
              "projects/app-ochibooh/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/app-ochibooh/tsconfig.app.json",
              "projects/app-ochibooh/tsconfig.spec.json",
              "projects/app-ochibooh/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/app-ochibooh/e2e/protractor.conf.js",
            "devServerTarget": "app-ochibooh:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "app-ochibooh:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "app-ochibooh"
}
